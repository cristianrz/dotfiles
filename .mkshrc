#!/bin/env ksh

# shellcheck disable=SC1090
[[ -f "$HOME/.env" ]] && . "$HOME/.env"

# shellcheck disable=SC1090
[[ -f ~/.bash_aliases ]] && . $HOME/.bash_aliases

# shellcheck disable=SC1090
[[ -f "$HOME/.functions" ]] && . "$HOME/.functions"

# shellcheck disable=SC1090
[[ -f "$HOME/.local-env" ]] && . "$HOME/.local-env"

# Uses the previously defined cd function when you open a terminal
cd . || exit
